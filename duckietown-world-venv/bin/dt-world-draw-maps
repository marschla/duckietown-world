#!/home/marco/dt-env-developer/src/duckietown-world/duckietown-world-venv/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'duckietown-world-daffy','console_scripts','dt-world-draw-maps'
__requires__ = 'duckietown-world-daffy'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('duckietown-world-daffy', 'console_scripts', 'dt-world-draw-maps')()
    )
